/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below
*/

/* Global */
@site       : 'positive';
@reset      : 'positive';

/* Elements */
@button     : 'positive';
@container  : 'positive';
@divider    : 'positive';
@emoji      : 'positive';
@flag       : 'positive';
@header     : 'positive';
@icon       : 'positive';
@image      : 'positive';
@input      : 'positive';
@label      : 'positive';
@list       : 'positive';
@loader     : 'positive';
@placeholder: 'positive';
@rail       : 'positive';
@reveal     : 'positive';
@segment    : 'positive';
@step       : 'positive';
@text       : 'positive';

/* Collections */
@breadcrumb : 'positive';
@form       : 'positive';
@grid       : 'positive';
@menu       : 'positive';
@message    : 'positive';
@table      : 'positive';

/* Modules */
@accordion  : 'positive';
@calendar   : 'positive';
@checkbox   : 'positive';
@dimmer     : 'positive';
@dropdown   : 'positive';
@embed      : 'positive';
@modal      : 'positive';
@nag        : 'positive';
@popup      : 'positive';
@progress   : 'positive';
@slider     : 'positive';
@rating     : 'positive';
@search     : 'positive';
@shape      : 'positive';
@sidebar    : 'positive';
@sticky     : 'positive';
@tab        : 'positive';
@toast      : 'positive';
@transition : 'positive';

/* Views */
@ad         : 'positive';
@card       : 'positive';
@comment    : 'positive';
@feed       : 'positive';
@item       : 'positive';
@statistic  : 'positive';

/*******************************
            Folders
*******************************/

/* Path to theme packages */
@themesFolder : 'themes';

/* Path to site override folder */
@siteFolder   : 'site/';


/*******************************
         Import Theme
*******************************/

@import (multiple) "theme.less";

/* End Config */
